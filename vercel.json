{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/search", "destination": "/search.html" },
    { "source": "/image/:slug", "destination": "/image.html" },

    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/dmca", "destination": "/dmca.html" },
    { "source": "/license", "destination": "/license.html" },
    { "source": "/contact", "destination": "/contact.html" },

    {
      "source": "/sitemap.xml",
      "destination": "https://pngfam-backend.onrender.com/sitemap.xml"
    },
    {
      "source": "/sitemap-images.xml",
      "destination": "https://pngfam-backend.onrender.com/sitemap-images.xml"
    }
  ]
}
